  "aboutPage": "About the app",
  "@aboutPage": {
    "description": "Label for about page",
    "type": "text"
  },
  "aboutVersion": "Version {version}",
  "@aboutVersion": {
    "description": "App version on the about page",
    "type": "text",
    "placeholders": {
      "version": {
        "type": "String",
        "example": "1.4.0"
      }
    }
  },
  "aboutLegalese": "Developed by {name}",
  "@aboutLegalese": {
    "description": "Developer attribution",
    "type": "text",
    "placeholders": {
      "name": {
        "type": "String",
        "example": "Xennis"
      }
    }
  },
  "imprint": "Legal notice",
  "@imprint": {
    "description": "Legal notice. The required \"Impressum\" due to German law.",
    "type": "text"
  },
  "imprintTmgText": "Information in accordance with ยง {paragraphNumber} TMG",
  "@imprintTmgText": {
    "description": "Disclaimer headline on the imprint page",
    "type": "text",
