  "optionPlannedAge": "Planned age",
  "@optionPlannedAge": {
    "description": "Age at death"
  },
  "optionDarkTheme": "Dark theme",
  "@optionDarkTheme": {
    "description": "Option to enforce the dark theme"
  },
  "optionDarkThemeDesc": "Enforce dark brightness",
  "@optionDarkThemeDesc": {
    "description": "Description for the option to enforce the dark theme"
  },
  "numberViewBirthToNow": "The clock shows the time from your birthday {birthday} to now.",
  "@numberViewBirthToNow": {
    "description": "Label for selecting clock from birthday to the current date",
    "type": "text",
    "placeholders": {
      "birthday": {
        "type": "DateTime",
        "format": "yMd",
        "example": "04/23/2001"
      }
    }
  },
  "numberViewNowToDeath": "The clock shows the time from now to your planned age of {age} ({deathDay}).",
  "@numberViewNowToDeath": {
    "description": "Label for selecting clock from current date to death",
    "type": "text",
    "placeholders": {
      "age": {
        "type": "int",
        "format": "decimalPattern",
        "example": "100"
      },
      "deathDay": {
        "type": "DateTime",
        "format": "yMd",
        "example": "04/23/2001"
      }
    }
  },
  "numberViewModeSwitch": "Count up",
  "@numberViewModeSwitch": {
    "description": "Label for switch between count up/down the lifetime."
  },
  "boxViewInfoText": "Every box represents 1 year. Blue for the past, green the future and red the current one.",
  "@boxViewInfoText": {
    "description": "Explains the user what is shown on the box view."
  },
  "firstOpenDialogTitle": "Hello",
  "@firstOpenDialogTitle": {
    "description": "Title of the dialog shown to a user opening the app for the first time."
  },
  "firstOpenDialogContent": "Before using the app set a birthday and age.",
  "@firstOpenDialogContent": {
    "description": "Text of the dialog shown to a user opening the app for the first time."
  },
  "firstOpenDialogOpenSettings": "Open Settings",
